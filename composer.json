{
    "name": "reason-digital/virgin-money-giving-api",
    "type": "library",
    "description": "Provides a PHP library for interacting with the Virgin Money Giving API. ",
    "keywords": ["API","VMG"],
    "homepage": "https://github.com/reason-digital/virgin-money-giving-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Reason Digital",
            "email": "hello@reasondigital.com",
            "homepage": "https://reasondigital.com"
        }
    ],
    "autoload": {
        "psr-4": { "VirginMoneyGivingAPI\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "~6.0",
        "rap2hpoutre/convert-accent-characters": "^0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "fzaninotto/faker": "^1.7",
        "php-coveralls/php-coveralls": "^2.1"
    }
}
